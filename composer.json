{
    "name": "arthurydalgo/inertia-laravel-crud-generator",
    "description": "Inertia Laravel CRUD Generator",
    "license": "MIT",
    "authors": [
        {
            "name": "zorca",
            "email": "vs@zorca.org",
            "homepage": "https://zorca.dev"
        }
    ],
    "homepage": "https://github.com/wpseed/inertia-laravel-crud-generator",
    "keywords": ["inertia", "laravel", "crud", "generator"],
    "require": {
        "illuminate/support": "^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6"
    },
    "autoload": {
        "psr-4": {
            "Wpseed\\InertiaLaravelCrudGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wpseed\\InertiaLaravelCrudGenerator\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wpseed\\InertiaLaravelCrudGenerator\\InertiaLaravelCrudGeneratorServiceProvider"
            ],
            "aliases": {
                "InertiaLaravelCrudGenerator": "Wpseed\\InertiaLaravelCrudGenerator\\Facades\\InertiaLaravelCrudGenerator"
            }
        }
    }
}
